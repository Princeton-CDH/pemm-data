{
    "profile": "tabular-data-package",
    "resources": [
        {
            "path": "data/canonical_story.csv",
            "profile": "tabular-data-resource",
            "name": "canonical_story",
            "format": "csv",
            "mediatype": "text/csv",
            "encoding": "utf-8",
            "schema": {
                "fields": [
                    {
                        "name": "Macomber ID",
                        "type": "string",
                        "format": "default",
                        "constraints": {
                            "required": true,
                            "unique": true,
                            "pattern": "^[1-9]\\d*(-[A-F][1-2]?)?$"
                        }
                    },
                    {
                        "name": "Macomber Title",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "Origin",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "English Translation",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "EMIP Summary",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "Hamburg Titles",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "Princeton Titles",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "Macomber Keywords",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "Notes",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "Earliest Attestation",
                        "type": "integer",
                        "format": "default",
                        "description": "The earliest century in which the story appeared."
                    },
                    {
                        "name": "Total Records",
                        "type": "integer",
                        "format": "default"
                    },
                    {
                        "name": "Total Paintings",
                        "type": "integer",
                        "format": "default"
                    },
                    {
                        "name": "Hamburg ID",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "CSM Number",
                        "type": "integer",
                        "format": "default",
                        "constraints": {
                            "minimum": 1,
                            "maximum": 420
                        }
                    },
                    {
                        "name": "Poncelet Number",
                        "type": "integer",
                        "format": "default",
                        "constraints": {
                            "minimum": 1,
                            "maximum": 1783
                        }
                    },
                    {
                        "name": "Clavis ID",
                        "type": "string",
                        "format": "default",
                        "constraints": {
                            "pattern": "^CAe [1-9]\\d{3}$"
                        }
                    },
                    {
                        "name": "Print Version",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "English Translation Link",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "English Translation Source",
                        "type": "string",
                        "format": "default"
                    },
                    {
                        "name": "Macomber ID Number",
                        "type": "integer",
                        "format": "default"
                    },
                    {
                        "name": "Macomber ID Letter",
                        "type": "string",
                        "format": "default"
                    }
                ],
                "missingValues": [
                    ""
                ]
            }
        },
